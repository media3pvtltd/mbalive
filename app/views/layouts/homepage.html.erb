    <div class="body-wrapper floatmenu">
        <!-- Header -->
        <%= render :partial => 'layouts/header' %>
        <!-- Layout -->
        <div class="content-wrapper" >
            <div class="gdlr-content">
                <%unless @title.nil?%>
                    <div class="gdlr-page-title-wrapper">
                        <div class="gdlr-page-title-overlay"></div>
                        <div class="gdlr-page-title-container container">
                            <h1 class="gdlr-page-title"><%= @title %></h1>
                        </div>  
                    </div>                
                <%end%>
                <div class="with-sidebar-wrapper" id="base-cont">
                    <%= yield %>
                </div>
            </div>
        </div>
        <%= render :partial => 'layouts/footer' %>
    </div>
        
    <%= javascript_include_tag 'application', media: 'all', 'data-turbolinks-track' => true     %>
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script>
        $(function() {
        $(window).scroll(function() {    
            var scroll = $(window).scrollTop();

            if (scroll >= 500) {
                $(".gdlr-header-inner").addClass("gdlr-header-fixed");
            } else {
                $(".gdlr-header-inner").removeClass("gdlr-header-fixed");
            }
        });
        });
    </script>
     <% [:notice, :error, :success, :warning].each do |n| %>
<%if flash[n] %>
<script>var n = noty({text: '<%= flash[n] %>',type: '<%=n%>', layout: 'topRight',animation: {
open: 'animated bounceInLeft', // Animate.css class names
close: 'animated bounceOutLeft', // Animate.css class names
easing: 'swing', // unavailable - no need
speed: 500
}}); </script>
<%end %>
<%end%>
<%unless flash['model_errors'].nil? %>
<%flash['model_errors'].each do |error| %>
<%error[1].each do |msg| %>
<script>var n = noty({text: '<%= error[0] %> <%= msg %>',type: 'error', layout: 'topRight',animation: {
open: 'animated bounceInRight', // Animate.css class names
close: 'animated bounceOutRight', // Animate.css class names
easing: 'swing', // unavailable - no need
speed: 500 // unavailable - no need
}}); </script>
<%end%>
<%end%>
<%end%>
