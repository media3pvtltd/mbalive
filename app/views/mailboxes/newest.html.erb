<div class="gdlr-lms-content">
<div class="gdlr-sidebar gdlr-right-sidebar two columns" >
		<div class="google_adsense skyscraper" style="padding-left: 18px;">
		 <div class="profile_blog_lft" style="border:none">
						<%= ad_unit(:wide_skyscraper, "pub-6430317842295107", {:slot => "5305317726"}) %>

						<%#= ad_unit(:wide_skyscraper, "pub-6430317842295107", {:slot => "5305317726"}) %>
						
		
						</div>
		</div>
		
	</div>
	<div class="one column"></div>
	<div class="seven columns">
			<div class="gdlr-item gdlr-tab-item horizontal"><div class="tab-title-wrapper"><a href="<%=mailboxes_mailbox_path %>"><h4 class="tab-title" ><i class="icon-inbox"></i><span>Inbox</span></h4></a><a href="<%=mailboxes_outbox_path %>"><h4 class="tab-title"><i class="icon-share-alt"></i><span>Outbox</span></h4></a><a href="<%=mailboxes_newest_path %>"><h4 class="tab-title active"><i class="icon-edit"></i><span>Compose</span></h4></a></div><div class="tab-content-wrapper">

					<i class="icon-chevron-left" ></i>
	<i class="icon-chevron-left" ></i><a href="<%=profile_index_path %>">Back to profile</a>
					<div class="tab-content active">
					<%=form_tag mailboxes_compose_path, :class =>'gdlr-item gdlr-content-item gdlr-lms-form' do |f| %>
					<input type="hidden" name="from" value=<%= @usermail %>>
					<input type="hidden" name="sender_at" value="TRUE">
					<input type="hidden" name="receiver_at" value="TRUE">
		<p>User id:*<br>
		<span id="error1" style="color:red"></span>
		<span class="wpcf7-form-control-wrap your-name">
		<%= text_field_tag 'name' , "",class: 'form-control' ,placeholder: 'Email' ,
		:id => "tags",  :autocomplete => "off"%>

		</span> </p>
		<p><select id="demo" style="" ><option value="">----select--------</option></select></p>
		<p>Subject<br>
		<span class="wpcf7-form-control-wrap your-subject">
			<%= text_field_tag 'subject' , "",class: 'form-control' ,placeholder: 'Subject' %>
		</span> </p>
		<p>Your Message<br>
		<span class="wpcf7-form-control-wrap your-message">
		<%= text_area_tag 'message' , "",class: 'form-control' ,placeholder: 'Message' %></span> </p>
		<p></p>
		<div class="wpcf7-response-output wpcf7-display-none"></div>
		<%= submit_tag 'Send',class: 'wpcf7-form-control wpcf7-submit',name: 'send',:onclick => 'return validatesform();' %>
		
		</div>
	<%end%>

			</div></div><div class="clear"></div></div>
			<div class="gdlr-sidebar gdlr-right-sidebar two columns" >
		<div class="google_adsense skyscraper" style="padding-left: 18px;">
		 <div class="profile_blog_lft" style="border:none">
						<%= ad_unit(:wide_skyscraper, "pub-6430317842295107", {:slot => "5305317726"}) %>

						<%#= ad_unit(:wide_skyscraper, "pub-6430317842295107", {:slot => "5305317726"}) %>
						
		
						</div>
		</div>
		
	</div>
	</div>
	
	<div class="clear"></div>
</div>
<div class="twelve column" style="text-align:center">
        <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- horizontal 2 -->
<ins class="adsbygoogle"
     style="display:inline-block;width:728px;height:90px"
     data-ad-client="ca-pub-6430317842295107"
     data-ad-slot="9645169322"></ins>
<script type="text/javascript">
	jQuery(document).ready(function(){
     
     $('#tags').keyup(function(){
            
            var email =$("#tags").val();
            // alert(email);
            $("#demo").css("display", "block");
            $.ajax({
                    url: "<%=root_url %>colleges/searchusers/"+email,
                      context: document.body
                    }).done(function(r) {
                    	// alert(r);
                            document.getElementById("demo").innerHTML = r;
                            
                        
                });
        });
     $('#demo').change(function(){
        var a=$('#demo').val();
        // alert(a);
        document.getElementById("tags").value = a;
        $("#demo").css("display", "none");
     })
});
</script>